<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="org.zerock.myapp.mapper.ReviewCommentMapper">
	<insert id="insert">
	<selectKey keyProperty="postNo" order="AFTER" resultType="int">
            <!-- select "ISEQ$$74653".NEXTVAL,"ISEQ$$_74653".CURRVAL from dual -->
            select "ISEQ$$_74709".CURRVAL from dual
      </selectKey>
	
	
    insert into tbl_ReviewComment (postNo,content, regiDate)
    values( #{postNo}, #{content}, current_Date)   
    
    </insert>
  </mapper>

  
