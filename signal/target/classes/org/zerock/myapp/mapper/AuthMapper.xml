<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.myapp.mapper.AuthMapper">

	<insert id="insertAuth">
		INSERT INTO tbl_users_auth (userid)
			 VALUES (#{userId})
	</insert>
	
	<select id="selectAuth">
		select * from tbl_users_auth where userid=#{userId}
	</select>
	
	<delete id="deleteAuth">
	
	</delete>
	
	<update id="updateAuth">
	
	</update>

</mapper>